{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\">\n                    {{#if is_active}}\n                    <strong>{{name}}</strong>\n                    {{else}}\n                    {{name}}\n                    {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n","components/common/custom-header-logo":"<div class=\"row header-logos logo-navv\">\r\n<div class=\"col-lg-4 col-md-4 col-sm-4 logo-nav\">\r\n    <a href=\"{{urls.home}}\" id=\"header-logo-block-image\">\r\n        {{#if settings.store_logo.image}}\r\n            <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n        {{else}}\r\n            <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n        {{/if}}\r\n    </a>\r\n    <div class=\"talk-live-header\" id=\"mobile-header-num\" style=\"display:none;\">\r\n        <span>Call us: </span><a onclick=\"gtag_report_conversion()\" href=\"tel:1800-997-3077\">1-800-997-3077</a>\r\n    </div>\r\n</div>\r\n    \r\n    <div class=\"col-lg-4 col-md-4 col-sm-4 tel-nav\">\r\n\r\n<div class=\"talk-live-first-div\">\r\n        <div class=\"talk-live-header\">\r\n            <h3>Talk to a Live Mobility Expert</h3>\r\n            <a onclick=\"gtag_report_conversion()\" href=\"tel:1800-997-3077\">1-800-997-3077</a>\r\n        </div>\r\n       \r\n        <img src=\"https://cdn11.bigcommerce.com/s-vz6unvhe9k/product_images/uploaded_images/header-customer-support.png?t=1612501014&_ga=2.123681227.1154238132.1612494366-2022119561.1571639426\">\r\n        \r\n</div>\r\n        <div class=\"customer-service-header\">\r\n            <p><span class=\"sales-header-text\">SALES:</span><a>8am - 9pm(Mon - Fri EST)</a></p>\r\n            <p>9am - 6pm(Sat - Sun EST)</p>\r\n            <p><span class=\"sales-header-text\">Customer Service:</span>9am - 5pm</p>\r\n            <p>(Mon - Fri EST)</p>\r\n        </div>\r\n\r\n\r\n    </div>\r\n\r\n <div class=\"col-lg-4 col-md-4 col-sm-4 search-nav\">\r\n     \r\n     <nav class=\"navUser navUser--left navUser--hasQuickSearch\">\r\n    {{> components/common/currency-selector}}\r\n\r\n    <ul class=\"navUser-section navUser-section--alt\">\r\n       \r\n        <li class=\"navUser-item\">\r\n            {{> components/emthemes-modez/search/quick-search-form css_class=\"navUser-action navUser-action--quickSearch\"}}\r\n        </li>\r\n        \r\n        {{#if theme_settings.navPages-show-cart}}\r\n        <li class=\"navUser-item navUser-item--cart hide-desktop\">\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                href=\"{{urls.cart}}\">\r\n                <span class=\"navUser-item-icon navUser-item-cartIcon emthemesModez-icon-handbag\"></span>\r\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n            </a>\r\n        </li>\r\n        {{else}}\r\n        <li class=\"navUser-item navUser-item--cart\">\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                data-dropdown=\"cart-preview-dropdown\"\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\">\r\n                <span class=\"navUser-item-icon navUser-item-cartIcon emthemesModez-icon-cart\"></span>\r\n                <span class=\"navUser-item-cartLabel\">Cart</span> <span class=\"countPill{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\r\n            </a>\r\n\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </li>\r\n        {{/if}}\r\n    </ul>\r\n\r\n    {{> components/emthemes-modez/search/quick-search-dropdown}}\r\n</nav>\r\n\r\n     \r\n    </div>\r\n\r\n    \r\n</div>\r\n","components/emthemes-modez/search/quick-search-dropdown":"<div class=\"dropdown dropdown--quickSearch visibleQuickSearch-results-wrapper\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\"\n    data-prevent-quick-search-close>\n    <section class=\"quickSearchResults visibleQuickSearch-results\" data-bind=\"html: results\"></section>\n</div>\n","components/emthemes-modez/search/quick-search-form":"<div class=\"{{#if css_class}}{{css_class}} {{/if}}visibleQuickSearch-form-container\" data-search=\"quickSearch\">\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query1\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input form-input--small\" data-search-quick name=\"search_query\" id=\"search_query1\" data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                    placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            </div>\n        </fieldset>\n    </form>\n</div>\n"}
