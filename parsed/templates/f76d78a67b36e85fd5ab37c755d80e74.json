{"components/common/custom-header-mobile-logo":"<div class=\"row header-logos logo-navv\">\r\n    <div class=\"col-lg-4 col-md-4 col-sm-4 logo-nav\">\r\n        <a href=\"{{urls.home}}\" id=\"header-logo-block-image\">\r\n            {{#if settings.store_logo.image}}\r\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n            {{else}}\r\n                <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\r\n            {{/if}}\r\n        </a>\r\n        <div class=\"talk-live-header\" id=\"mobile-header-num\" style=\"display:none;\">\r\n            <span>Call us: </span><a href=\"tel:1800-997-3077\">1-800-997-3077</a>\r\n        </div>\r\n    </div>   \r\n</div>\r\n<div class=\"navigation-sticky-search\">\r\n    <div class=\"{{#if css_class}}{{css_class}} {{/if}}visibleQuickSearch-form-container\" data-search=\"quickSearch\">\r\n        <form class=\"form\" action=\"{{urls.search}}\">\r\n            <fieldset class=\"form-fieldset\">\r\n                <div class=\"form-field\">\r\n                    <label class=\"is-srOnly\" for=\"search_queryA\">{{lang \"search.quick_search.input_label\"}}</label>\r\n                    <input class=\"form-input form-input--small\" data-search-quick name=\"search_query\" id=\"search_queryA\" data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                        placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n                </div>\r\n            </fieldset>\r\n        </form>\r\n    </div>\r\n</div>\r\n"}
